{
    "version": 2,
    "builds": [
      {
        "src": "server2.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/server2.js" },
      { "src": "/register", "dest": "/server2.js" },
      { "src": "/topic", "dest": "/server2.js" },
      { "src": "/subtopic", "dest": "/server2.js" },
      { "src": "/detail", "dest": "/server2.js" },
      { "src": "/detail2", "dest": "/server2.js" },
      { "src": "/register/(.*)", "dest": "/server2.js" },
      { "src": "/topic/(.*)", "dest": "/server2.js" },
      { "src": "/subtopic/(.*)", "dest": "/server2.js" },
      { "src": "/detail/(.*)", "dest": "/server2.js" },
      { "src": "/detail2/(.*)", "dest": "/server2.js" }
    ]
  }
  